{
    "name": "sprintcube/taskueue",
    "description": "Taskueue plugin for CakePHP",
    "type": "cakephp-plugin",
    "keywords": ["cakephp", "queue", "tasks", "background"],
	"homepage": "https://github.com/sprintcube/cakephp-taskueue",
    "license": "MIT",
    "authors": [
		{
			"name": "Narendra Vaghela",
			"homepage": "https://www.sprintcube.com",
			"role": "Maintainer"
		},
		{
			"name": "Community Contributors",
			"homepage": "https://github.com/sprintcube/cakephp-taskueue/graphs/contributors"
		}
	],
	"support": {
		"issues": "https://github.com/sprintcube/cakephp-taskueue/issues",
		"source": "https://github.com/sprintcube/cakephp-taskueue"
	},
    "require": {
        "php": ">=7.2",
        "cakephp/cakephp": "^4.0",
        "josegonzalez/cakephp-queuesadilla": "^0.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Taskueue\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Taskueue\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
